        <CheckBox x:Name="check1" Content="CheckBox" HorizontalAlignment="Left" Margin="210,130,0,0" VerticalAlignment="Top" FontSize="16" Height="21" Width="145" BorderBrush="#FF1F64C7" FontWeight="Bold" FontFamily="Stencil" Background="#FFF5EDED">
            <CheckBox.Foreground>
                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                    <GradientStop Color="Black" Offset="0"/>
                    <GradientStop Color="#FF6E5F5F" Offset="1"/>
