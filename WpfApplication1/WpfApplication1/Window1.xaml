                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                    <GradientStop Color="Black" Offset="0"/>
                    <GradientStop Color="#FF6E5F5F" Offset="1"/>
                </LinearGradientBrush>
            </CheckBox.Foreground>
        </CheckBox>
        <Label Content="Prüfungsbogennummer" HorizontalAlignment="Left" Margin="0,10,0,0" VerticalAlignment="Top"/>
        <Label Content="6" HorizontalAlignment="Left" Margin="141,10,0,0" VerticalAlignment="Top"/>
        <ProgressBar HorizontalAlignment="Left" Height="19" Margin="111,266,0,0" VerticalAlignment="Top" Width="212"/>
        <Label Content="Ende" HorizontalAlignment="Left" Margin="340,259,0,0" VerticalAlignment="Top" RenderTransformOrigin="-0.028,0.538"/>
